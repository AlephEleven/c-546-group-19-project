<h1>My Clothes</h1>
<div>
    <div>{{error}}</div>
    <a href="/clothes/new">Add +</a>
    <br>
    {{#each clothingItems}}
        <div class="form-div clothing-item-card">
            <img class="clothing-item-image" alt="{{name}}" src="/uploads/{{image}}">
            <p>{{name}}</p>
            <p>Type: {{type}}</p>
        </div>
    {{/each}}
</div>